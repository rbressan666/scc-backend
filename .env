# Configurações do Servidor
PORT=3000
NODE_ENV=development

# Configurações do Banco de Dados PostgreSQL (Supabase)
# IMPORTANTE: Substitua pelos dados reais do seu projeto Supabase
DATABASE_URL=postgresql://postgres:password@localhost:5432/scc_mvp1

# Configurações JWT
JWT_SECRET=scc-super-secret-jwt-key-development-only-change-in-production
JWT_EXPIRES_IN=24h

# Configurações CORS
CORS_ORIGIN=http://localhost:3001

# Configurações de Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# URL do servidor (para QR Code)
SERVER_URL=http://localhost:3000

